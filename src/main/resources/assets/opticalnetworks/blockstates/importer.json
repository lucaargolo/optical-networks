{
  "multipart": [

    {"when": {"north": "false"}, "apply": { "model": "opticalnetworks:block/cable_face"}},
    {"when": {"south": "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 180}},
    {"when": {"east" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 90}},
    {"when": {"west" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 270}},
    {"when": {"up"   : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "x": -90}},
    {"when": {"down" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "x": 90}},

    {"when": {"north": "true"}, "apply": { "model": "opticalnetworks:block/cable_connection"}},
    {"when": {"south": "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 180}},
    {"when": {"east" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 90}},
    {"when": {"west" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 270}},
    {"when": {"up"   : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "x": -90}},
    {"when": {"down" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "x": 90}},

    {"when": {"facing" : "north"}, "apply": { "model": "opticalnetworks:block/importer"}},
    {"when": {"facing" : "south"}, "apply": { "model": "opticalnetworks:block/importer", "y": 180}},
    {"when": {"facing" : "east"}, "apply": { "model": "opticalnetworks:block/importer", "y": 90}},
    {"when": {"facing" : "west"}, "apply": { "model": "opticalnetworks:block/importer", "y": 270}},
    {"when": {"facing" : "up"}, "apply": { "model": "opticalnetworks:block/importer", "x": -90}},
    {"when": {"facing" : "down"}, "apply": { "model": "opticalnetworks:block/importer", "x": 90}}

  ]
}