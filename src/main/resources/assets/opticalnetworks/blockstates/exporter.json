{
  "multipart": [

    {"when": {"north": "false"}, "apply": { "model": "opticalnetworks:block/cable_face"}},
    {"when": {"south": "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 180}},
    {"when": {"east" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 90}},
    {"when": {"west" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "y": 270}},
    {"when": {"up"   : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "x": -90}},
    {"when": {"down" : "false"}, "apply": { "model": "opticalnetworks:block/cable_face", "x": 90}},

    {"when": {"north": "true"}, "apply": { "model": "opticalnetworks:block/cable_connection"}},
    {"when": {"south": "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 180}},
    {"when": {"east" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 90}},
    {"when": {"west" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "y": 270}},
    {"when": {"up"   : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "x": -90}},
    {"when": {"down" : "true"}, "apply": { "model": "opticalnetworks:block/cable_connection", "x": 90}},

    {"when": {"facing" : "north", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter"}},
    {"when": {"facing" : "south", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter", "y": 180}},
    {"when": {"facing" : "east", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter", "y": 90}},
    {"when": {"facing" : "west", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter", "y": 270}},
    {"when": {"facing" : "up", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter", "x": -90}},
    {"when": {"facing" : "down", "enabled" : "true"}, "apply": { "model": "opticalnetworks:block/exporter", "x": 90}},

    {"when": {"facing" : "north", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off"}},
    {"when": {"facing" : "south", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off", "y": 180}},
    {"when": {"facing" : "east", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off", "y": 90}},
    {"when": {"facing" : "west", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off", "y": 270}},
    {"when": {"facing" : "up", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off", "x": -90}},
    {"when": {"facing" : "down", "enabled" : "false"}, "apply": { "model": "opticalnetworks:block/exporter_off", "x": 90}}

  ]
}